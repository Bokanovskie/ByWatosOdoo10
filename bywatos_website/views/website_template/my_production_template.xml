<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <template id="assets_frontend_production" inherit_id="website.assets_frontend" name="production">
        <xpath expr="." position="inside">
            <link rel="stylesheet" href="/bywatos_website/static/src/css/production.css"></link>
        </xpath>
    </template>

    <!-- My Production Template View -->
    <template id="my_production_template_view">
        <t t-call="website.layout">
            <div id="portfolio" class="container">
                <div class="row">
                    <t t-foreach="product_ids" t-as="product">
                        <div class="col-md-4 container-image">
                            <a href="">
                                <img t-attf-src="/website/image/product.template/{{product.id}}/image/450x400"
                                     class="image-product"/>
                            </a>

                            <span t-esc="product.name" />
                        </div>
                    </t>
                </div>
            </div>
        </t>
    </template>
</odoo>