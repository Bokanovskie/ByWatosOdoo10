    <!-- My Production Template View -->
    <template id="my_production_template_view">
        <t t-call="website.layout">
            <div id="wrap">
                <t t-call="bywatos_website_theme.layout_aside_bar"/>
                <!--#main-wrap-->
                <div id="main-wrap">
                    <div id="main">
                        <div id="content">
                            <div id="post-122" class="page type-page status-publish hentry">
                                <div class="row-fluid">
                                    <div id="content_wrap" class="">
                                        <div class="content-wrap-inn ">
                                            <div class="pagebuilder-wrap">
                                                <div class="fullwrap_moudle">
                                                    <div class="row-fluid">
                                                        <div id="full" class="fullwidth-wrap bg-theme-color-10">
                                                            <div class="row-fluid">
                                                                <div class="span12 moudle bottom-space-no">
                                                                    <!--Portfolio isotope-->
                                                                    <div class="row-fluid">
                                                                        <!--Filter-->
                                                                        <div class="clearfix filters filter-floating filter-floating-fixed">
                                                                            <ul>
                                                                                <li class="active"><a href="#" data-filter="*">All</a></li>
                                                                                <li><a data-filter=".filter_my_creation" href="#">Mes cr√©ation</a></li>
                                                                                <li><a data-filter=".filter_requests" href="#">Les demandes</a></li>
                                                                            </ul>
                                                                            <div class="filter-floating-triggle hidden-phone"><i class="fa fa-filter"></i></div>
                                                                        </div>
                                                                        <!--End filter-->
                                                                        <div class="container-isotope" data-post="20140402-063639-867">
                                                                            <div class="isotope masonry isotope_fade" data-space="1px" data-size="brick" style="margin: -1px 0px 0px -1px;">
                                                                                <t t-foreach="product_ids" t-as="product">
                                                                                    <!--Item-->
                                                                                    <div class="width-and-small isotope-item brick-with-img">
                                                                                        <div class="inside brick-inside post-bgcolor-default" style=" margin:1px 0 0 1px;">
                                                                                            <a t-att-href="'/shop/product/%s' % slug(product)" t-att-title="product.name" class="brick-link">
                                                                                                <div class="brick-hover-mask brick-hover">
                                                                                                    <h3 class="brick-title"><t t-esc="product.name"/></h3>
                                                                                                </div>
                                                                                                <div class="brick-content">
                                                                                                    <!--class="attachment-imagebox-thumb wp-post-image" alt="Paper-Hot-Cup-Mock-Up-vol-2"/>-->
                                                                                                    <!--<a t-att-href="'/shop/product/%s' % slug(product)"></a>-->
                                                                                                    <img t-attf-src="/website/image/product.product/{{product.product_variant_id.id}}/image/"
                                                                                                         class="attachment-imagebox-thumb wp-post-image" t-att-alt="product.name"/>
                                                                                                </div>
                                                                                            </a>
                                                                                        </div>
                                                                                    </div>
                                                                                </t>

                                                                            </div>
                                                                        </div>
                                                                        <!--End container-isotope-->
                                                                    </div>
                                                                    <!--End row-fluid-->
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <script type="text/javascript">
                                                    jQuery(document).ready(function(){
                                                    var ux_pb = new ThemePageBuilder();
                                                    ux_pb.init();
                                                    });
                                                </script>
                                            </div>
                                        </div>
                                        <!--End content-wrap-inn-->
                                    </div>
                                    <!--end content_wrap-->
                                </div>
                            </div>
                            <!--end post-->
                        </div>
                        <!--end content-->
                    </div>
                    <!--End #main-->
                </div>
                <!--End #main-wrap-->
            </div><!--End #wrap-->

            <!-- Float bar -->
            <div id="float-bar" class="hidden-phone">
                <div class="float-bar-inn-wrap">
                    <div class="float-bar-inn">
