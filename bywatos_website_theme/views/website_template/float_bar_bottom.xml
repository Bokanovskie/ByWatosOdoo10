<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <template id="float_bar_social_network" name="Float Bar Social Network">
        <!-- Float bar -->
        <div id="float-bar" class="hidden-phone">
            <div class="float-bar-inn-wrap">
                <div class="float-bar-inn">
                    <!--Social network share icon-->
                    <div class="float-bar-social-share">
                        <a t-if="website.social_facebook" t-att-href="website.social_facebook" target="_blank">
                            <button class="social-share-facebook">
                                <i class="fa fa-facebook"></i>
                            </button>
                        </a>

                        <a t-if="website.social_twitter" t-att-href="website.social_twitter" target="_blank">
                            <button class="social-share-twitter">
                                <i class="fa fa-twitter"></i>
                            </button>
                        </a>

                        <a  t-if="website.social_linkedin" t-att-href="website.social_linkedin" target="_blank">
                            <button class="social-share-linkedin">
                                <i class="fa fa-linkedin"></i>
                            </button>
                        </a>

                        <a  t-if="website.social_youtube"  t-att-href="website.social_youtube" target="_blank">
                            <button class="social-share-youtube">
                                <i class="fa fa-youtube-square"></i>
                            </button>
                        </a>

                        <a  t-if="website.social_googleplus"  t-att-href="website.social_googleplus" target="_blank">
                            <button class="social-share-google-plus">
                                <i class="fa fa-google-plus"></i>
                            </button>
                        </a>

                        <a  t-if="website.social_instagram"  t-att-href="website.social_instagram" target="_blank">
                            <button class="social-share-google-plus">
                                <i class="fa fa-instagram"></i>
                            </button>
                        </a>
                    </div>
                </div>
                <!--End float inn wrap-->
            </div><!--End float inn-->
            <div id="float-bar-triggler"><i class="float-bar-triggler-inn"></i></div>
        </div>
        <!--End #float bar-->
    </template>
</odoo>