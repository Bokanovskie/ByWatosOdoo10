<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="subscribers_news_letter_tree_view" model="ir.ui.view">
        <field name="name">Abonnées News Letter Tree View</field>
        <field name="model">mail.subscribers</field>
        <field name="arch" type="xml">
            <tree string="Abonnées News Letter">
                <field name="mail"/>
            </tree>
        </field>
    </record>

    <record id="action_subscribers_website_news_letter" model="ir.actions.act_window">
        <field name="name">Abonnées News Letter</field>
        <field name="res_model">mail.subscribers</field>
        <field name="view_mode">tree,form</field>
    </record>

</odoo>